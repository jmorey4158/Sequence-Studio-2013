  xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" 
  xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10" xmlns:MSHelp="http://msdn.microsoft.com/mshelp">
  
  <command:details>
    <command:name>Get-BestMatchesPercentage</command:name>
    <maml:description>
      <maml:para>Returns a MatchSet representing the best match by percentage of all matches between the two sequences.</maml:para>
    </maml:description>
    <maml:copyright>Copyright 2013 - Revealed Designs LLC</maml:copyright>
    <command:verb>Get</command:verb>
    <command:noun>BestMatchesPercentage</command:noun>
    <dev:version />
  </command:details>
  
  <maml:description>
    <maml:para>The Get-BestMatchesPercentage cmdlet returns a MatchSet representing the best match by percentage of all matches between the two sequence strings. You can specify the minimum percentage for a valid match using 'threshold'.</maml:para>
  </maml:description>
  
  <command:syntax>
    <command:syntaxItem>
      <maml:name>Get-BestMatchesPercentage</maml:name>
      <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="true" position="named" aliases="s">
        <maml:name>Sequence</maml:name>
        <maml:description><maml:para>The sequence string to be compared against the other sequence string - 'Pattern'.</maml:para></maml:description>
      </command:parameter>
      <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="true" position="named" aliases="p">
        <maml:name>Pattern</maml:name>
        <maml:description><maml:para>The sequence string to be compared against the other sequence string - 'Sequence'.</maml:para></maml:description>
      </command:parameter>
    </command:syntaxItem>  
    
    <command:syntaxItem>  
      <maml:name>Get-BestMatchesPercentage</maml:name>
      <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="true" position="named" aliases="s">
        <maml:name>Object</maml:name>
        <maml:description><maml:para>A DNA, RNA, or Polypeptide class instance that contains the sequence string to be compared against the other sequence string - 'Pattern'.</maml:para></maml:description>
      </command:parameter>
      <command:parameter required="true" variableLength="true" globbing="false" pipelineInput="true" position="named" aliases="p">
        <maml:name>Pattern</maml:name>
        <maml:description><maml:para>The sequence string to be compared against the other sequence string - 'Object'.</maml:para></maml:description>
      </command:parameter>      
    </command:syntaxItem>       
  </command:syntax>
  
